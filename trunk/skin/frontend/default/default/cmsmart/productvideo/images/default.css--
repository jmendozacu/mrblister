/* STYLE 01: TAB */
#productvideo-tab-menu{
	height: 30px;
    line-height: 30px;
	margin: 0 0 -1px;
}
#productvideo-tab-menu .productvideo-tab-item{
	background: url("tab-menu-bg.jpg") repeat-x scroll 0 bottom rgba(0, 0, 0, 0);
    border: 1px solid #C7C7C7;
    border-bottom: none;
    cursor: pointer;
    display: block;
    float: left;
    font-size: 14px;
    font-weight: normal;
    height: 28px;
    line-height: 28px;
    margin: 0 3px 0 0;
    min-width: 100px;
    text-align: center;
    z-index: 0;
}
#productvideo-tab-menu .productvideo-tab-item:hover{
	/*background: none repeat scroll 0 0 #999999;*/
}
#productvideo-tab-menu .productvideo-tab-item.selected {
	background: none repeat scroll 0 0 #FFFFFF;
    color: #DA6201;
    height: 29px
}
#productvideo-tab-video-content-wrapper{
	border: 1px solid #C7C7C7;
    -moz-box-shadow: -1px 1px 2px 0 rgba(206, 206, 206, 0.5), 1px 1px 2px 0 rgba(206, 206, 206, 0.5);
    -webkit-box-shadow: -1px 1px 2px 0 rgba(206, 206, 206, 0.5), 1px 1px 2px 0 rgba(206, 206, 206, 0.5);
    box-shadow: -1px 1px 2px 0 rgba(206, 206, 206, 0.5), 1px 1px 2px 0 rgba(206, 206, 206, 0.5);
}
.product-img-box .productvideo-tab-content{
	/*border: 1px solid #2F5EA2;
	-moz-box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);
	-webkit-box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);*/
}
.product-img-box .productvideo-tab-content .product-image{
	border-bottom: 1px solid #C7C7C7;
}
#productvideo-tab-video-content{
	display: none;
}
#productvideo-tab-video-content #productvideo-tab-video-frame{
	border-bottom: 1px solid #C7C7C7;
    display: block;
    margin: 0;
    padding: 10px 10px 15px;
}
.product-img-box .productvideo-tab-content .more-views h2{
}
.product-img-box  .more-views{
	margin: 10px 10px 0;
	position: relative;
}
 .product-img-box .productvideo-tab-content .more-views{ 
	display: inline-block;
	padding: 0 40px;
}
.product-img-box .productvideo-tab-content .more-views li>a{
	border: 0 none;
	height: auto;
    position: relative;
    width: auto;
}
.product-img-box .productvideo-tab-content .more-views li>a:hover{
	border: 0 none;
}
.product-img-box .productvideo-tab-content .more-views li>a.cmsmart-thumbnail-selected{
	border: 0;
	/*-moz-box-shadow: 0 0 2px rgba(0, 0, 0, 0.7);
	-webkit-box-shadow: 0 0 2px rgba(0, 0, 0, 0.7);
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.7);*/
}
.product-img-box .productvideo-tab-content .more-views li>a>img{
}
/* END STYLE 01: TAB */

