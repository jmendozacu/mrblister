/* slider */
.more-views .box-nav{
	position:absolute;
	width:100%;
	bottom:-50%;
}
.more-views .box-nav a{
	border: 1px solid #c7c7c7;
	height: 55px;
	position: relative;
	top: -30px;
	width: 18px;
}
.more-views .box-nav a.next{
	right:0;
	float:right;
	background:url("next-button.png") no-repeat center center;
}
.more-views .box-nav a.prev{
	left:-11px;
	float:left;
	background:url("back-button.png") no-repeat center center;
}

