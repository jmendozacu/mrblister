/* POPUP STYLE */
#productvideo-popup-wrapper{
	background: none repeat scroll 0 0 rgba(0, 0, 0, 0.5);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 999;
	display: none;
}
#productvideo-popup-content{
	/*-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
    border-radius: 10px;
	-moz-box-shadow: 0 0 10px rgba(255, 255, 255, 0.7);
	-webkit-box-shadow: 0 0 10px rgba(255, 255, 255, 0.7);
    box-shadow: 0 0 10px rgba(255, 255, 255, 0.7);*/
	background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #C7C7C7;
    display: none;
    left: 50%;
    min-height: 20px;
    min-width: 200px;
    /* padding: 20px 10px; *//* fix style for 1.9.x */
	padding: 20px 0px;
    position: absolute;
    top: 100px;
    z-index: 999999999999;
}
#productvideo-productimg-ajaxloading,
#productvideo-popup-ajaxloading{
	background: url("ajax-loader.gif") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
    display: inline-block;
    height: 19px;
    left: 50%;
    margin: -10px 0 0 -110px;
    position: absolute;
    top: 50%;
    width: 220px;
	z-index: 9;
}
#productvideo-productimg-ajaxloading{
    display: none;
}
#productvideo-popup-content-ajax{
}
#productvideo-popup-display-item{
	margin: 0 30px;
}
.productvideo-popup-list-item-title{
	border-bottom: 1px solid #C7C7C7;
    color: #DA6201;
    font-size: 14px;
    margin: 0 0 5px;
    padding: 10px 29px 5px;
}
.bottom_popup .productvideo-popup-list-item-title{
    color: #000000;
}
.bottom_popup .productvideo-popup-list-item-title.selected{
    color: #DA6201;
}
.productvideo-popup-list-item{
	/*-moz-box-shadow: 0 0 5px rgba(84, 84, 84, 0.7);
	-webkit-box-shadow: 0 0 5px rgba(84, 84, 84, 0.7);
    box-shadow: 0 0 5px rgba(84, 84, 84, 0.7);
    border-top: 1px solid #C7C7C7;*/
    margin: 10px 0 0;
    padding: 0;
}
.productvideo-popup-list-item .productvideo-popup-item{	
	border: 1px solid #C7C7C7;
    cursor: pointer;
    display: inline-block;
    margin: 5px 10px 5px 0;
    position: relative;
}
.productvideo-popup-list-item .productvideo-popup-item:hover{
	border: 1px solid #999999;
}
.productvideo-popup-list-item .productvideo-popup-item.selected{
	border: 1px solid #F18200;
}
#productvideo-popup-close-button{
	background: url("popup-close-buttotn.png") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
    cursor: pointer;
    display: none;
    height: 10px;
    position: absolute;
    right: 5px;
    top: 5px;
    width: 10px;
}
/* END POPUP STYLE */


/* IMAGE SLIDER STYLE */
.productvideo-image-slider-wrapper{ 
    position: relative;
}
.productvideo-image-slider-wrapper>ul{
	padding:0 15px;
} 
.productvideo-image-slider-button{
    border: 1px solid #C7C7C7;
    cursor: pointer;
    display: none;
    margin: 2px 0;
    position: absolute;
	width: 18px;
}
.productvideo-image-slider-button.disable{
	-moz-opacity: 0.4;
	-webkit-opacity: 0.4;
	-ms-filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=40);
    opacity: 0.4;
}
.productvideo-image-slider-button:hover{
    border: 1px solid #999999;
}
.productvideo-image-slider-button.disable:hover{
    border: 1px solid #C7C7C7;
}
.productvideo-image-slider-back-button{
	left: 0;
	top: 0;
	background: url("back-button.png") no-repeat scroll center center rgba(0, 0, 0, 0);
}
.productvideo-image-slider-back-button:hover{
	background: url("back-button-hover.png") no-repeat scroll center center rgba(0, 0, 0, 0);
}
.productvideo-image-slider-back-button.disable:hover{
	background: url("back-button.png") no-repeat scroll center center rgba(0, 0, 0, 0);
}
.productvideo-image-slider-next-button{
	/* right: 0; */ /* fix style for 1.9.x */
	right: 3px;
	top: 0;
	background: url("next-button.png") no-repeat scroll center center rgba(0, 0, 0, 0);
}
.productvideo-image-slider-next-button:hover{
	background: url("next-button-hover.png") no-repeat scroll center center rgba(0, 0, 0, 0);
}
.productvideo-image-slider-next-button.disable:hover{
	background: url("next-button.png") no-repeat scroll center center rgba(0, 0, 0, 0);
}
/* Popup Thumbnail Slide */
.productvideo-image-slider-content-wrapper{
	position: relative;
}
#productvideo-popup-content .productvideo-image-slider-button.productvideo-image-slider-back-button,
#productvideo-popup-content .productvideo-image-slider-button.productvideo-image-slider-next-button{
	bottom: 0;
	top: auto;
}
#productvideo-popup-content .productvideo-image-slider-wrapper{
	margin: 0 30px;
}
#productvideo-popup-content .productvideo-image-slider-wrapper .productvideo-image-slider{
}
#productvideo-popup-content .productvideo-image-slider-wrapper .productvideo-image-slider .productvideo-popup-item{
	margin: 0 10px 0 0;
}
/* End Popup Thumbnail Slide */

